{
    "name": "kr0lik/phpstan-rules",
    "type": "library",
    "description": "Provides additional rules for phpstan/phpstan.",
    "require": {
        "php": ">=7.4",
        "ergebnis/phpstan-rules": "^0.15.0 || ^1.0",
        "pepakriz/phpstan-exception-rules": "^0.11.0 || ^0.12.0",
        "phpstan/phpstan": "^0.12.0 || ^1.0",
        "phpstan/phpstan-phpunit": "^0.12.0 || ^1.0",
        "phpstan/phpstan-strict-rules": "^0.12.0 || ^1.0",
        "phpstan/phpstan-symfony": "^0.12.0 || ^1.0",
        "slam/phpstan-extensions": "^5.0 || ^6.0",
        "thecodingmachine/phpstan-strict-rules": "^0.12.0 || ^1.0"
    },
    "require-dev": {
        "kr0lik/php-cs-fixer": "^2.0",
        "icanhazstring/composer-unused": "^0.8",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "bin-dir": "bin",
        "sort-packages": true,
        "allow-plugins": {
            "icanhazstring/composer-unused": true
        }
    },
    "autoload": {
        "psr-4": {
            "kr0lik\\PHPStanRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kr0lik\\PHPStanRules\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "unused": [
            "ergebnis/phpstan-rules",
            "pepakriz/phpstan-exception-rules",
            "slam/phpstan-extensions",
            "thecodingmachine/phpstan-strict-rules"
        ]
    }
}
